{
  "env": {
    "browser": true,
    "node": true
  },
  "extends": ["airbnb", "prettier"],
  "rules": {
    "react/jsx-filename-extension": "off",
    "react/jsx-one-expression-per-line": "off",
    "react/jsx-wrap-multilines": "off"
  },
  "settings": {
    "import/core-modules": ["jss", "react-jss", "styled-jsx"]
  }
}
